 	   <select id="selectBoardList" parameterType="hashmap" resultType="hashmap">
        <![CDATA[
            SELECT
                IDX,
                TITLE,
                HIT_CNT
            FROM
                TB_BOARD
            WHERE
                DEL_GB = 'N'
            ORDER BY IDX DESC
